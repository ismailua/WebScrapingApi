{
  "name": "ismailcakir/webscrapingapi",
  "description": "Web scraping API client for Laravel",
  "type": "library",
  "autoload": {
    "psr-4": {
      "WebScrapingApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WebScrapingApi\\WebScrapingApiServiceProvider"
      ],
      "aliases": {
        "WebScrapingApi": "WebScrapingApi\\Facades\\WebScrapingApi"
      }
    }
  },
  "require": {
    "php": ">=8.0",
    "symfony/http-client": "^7.2"
  }
}